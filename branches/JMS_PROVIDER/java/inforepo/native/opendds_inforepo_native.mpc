project: dcps, optional_jni_check {
    sharedname       = opendds-inforepo-native

    includes        += $(JAVA_HOME)/include $(JAVA_HOME)/include/$(JAVA_PLATFORM) \
                       $(ACE_ROOT) $(TAO_ROOT)/orbsvcs
    
    after           += InfoRepoLib Federator DCPSInfoRepoServ
    libs            += DCPSInfoRepoServ InfoRepoLib Federator

    libout           = $(DDS_ROOT)/lib

    dynamicflags    += OPENDDS_INFOREPO_NATIVE_BUILD_DLL
}
