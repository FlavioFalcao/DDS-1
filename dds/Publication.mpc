project : orbsvcslib, core, portableserver {

  requires    += exceptions

  sharedname   = TAO_DdsDcps_Publication
  dynamicflags = TAO_DDSDCPS_PUBLICATION_BUILD_DLL
  libout       = $(DDS_ROOT)/lib
