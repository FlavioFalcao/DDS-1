<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.opendds.modeling.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            category="org.opendds.modeling.Wizard.category.ID"
            class="org.opendds.modeling.model.OpenDDS.presentation.OpenDDSModelWizard"
            icon="icons/full/obj16/OpenDDSModelFile.gif"
            id="OpenDDS.presentation.OpenDDSModelWizardID"
            name="%_UI_OpenDDSModelWizard_label">
         <description>%_UI_OpenDDSModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
