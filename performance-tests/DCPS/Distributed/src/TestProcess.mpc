project(dtpIdl): dcps {
  // This project ensures the common components get built first.

  TypeSupport_Files {
    Test.idl
  }

  custom_only = 1
}


project(dtpProcess) : dcpsexe_with_unreliable_dgram, dcps_reliable_multicast, dcps_tcp {
  exename = testprocess
  exeout  = ../bin
  after  += dtpIdl

  TypeSupport_Files {
    Test.idl
  }

  Source_Files {
    Writer.cpp
    Publisher.cpp
    Subscriber.cpp
    Options.cpp
    PublicationProfile.cpp
    DataReaderListener.cpp
    testprocess.cpp
  }
}

