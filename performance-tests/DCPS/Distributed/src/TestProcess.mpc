project(dtpIdl): dcps {
  // This project ensures the common components get built first.

  TypeSupport_Files {
    Test.idl
  }

  custom_only = 1
}


project(dtpProcess) : dcpsexe_with_unreliable_dgram, dcps_reliable_multicast, dcps_tcp {
  exename = testprocess
  exeout  = ../bin
  after  += dtpIdl

  TypeSupport_Files {
    Test.idl
  }

  Source_Files {
    Exponential.cpp
    Gaussian.cpp
    EntityProfiles.cpp
    Options.cpp
    Publication.cpp
    // Subscription.cpp
    DataReaderListener.cpp
    Process.cpp
    testprocess.cpp
  }
}

