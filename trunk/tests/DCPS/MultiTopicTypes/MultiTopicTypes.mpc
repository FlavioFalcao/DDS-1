project : dcps {
  after       += MultiTopicTypes
  libout       = .

  idlflags    += -Wb,export_macro=FooLib_Export -Wb,export_include=foolib_export.h
  dynamicflags = FOOLIB_BUILD_DLL
  pch_header   =
  pch_source   =
  dcps_ts_flags += --export=FooLib_Export

  TypeSupport_Files {
    Foo1Def.idl
    Foo2Def.idl
    Foo3Def.idl
  }

  IDL_Files {
    Foo1Def.idl
    Foo1DefTypeSupport.idl
    Foo2Def.idl
    Foo2DefTypeSupport.idl
    Foo3Def.idl
    Foo3DefTypeSupport.idl
  }
  
  Source_Files {
    Foo1DefTypeSupportImpl.cpp
    Foo2DefTypeSupportImpl.cpp
    Foo3DefTypeSupportImpl.cpp
  }
}

